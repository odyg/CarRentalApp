@model CarRentalApp.Models.RenterModel

@{
    ViewData["Title"] = "Add New Renter";
}

<h2>@ViewData["Title"]</h2>

<form asp-action="AddRenter" method="post" asp-controller="Renter">
    <div class="mb-1">
        <label asp-for="FName"></label>
        <input asp-for="FName" />
    </div>
    <div class="mb-1">
        <label asp-for="LName"></label>
        <input asp-for="LName" />
    </div>
	<div class="mb-1">
		<label asp-for="Address"></label>
		<input asp-for="Address" />
    </div>
    <div class="mb-1">
        <label asp-for="PhoneNumber"></label>
        <input asp-for="PhoneNumber" />
    </div>
    <div class="mb-1">
        <label asp-for="Email"></label>
        <input asp-for="Email" />
    </div>
    <div class="mb-1">
        <label asp-for="ZipCode"></label>
        <input asp-for="ZipCode" />
    </div>
   
    <!-- Repeat for other fields -->
    <div class="mb-1">
       <input type="submit" value="Add Renter" />
    </div>
</form>